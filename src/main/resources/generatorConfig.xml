<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration 
	PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
	"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>

	<context id="default" targetRuntime="MyBatis3" defaultModelType="flat">
		<!-- for enabling plugins need move plugins to separate module
			and in generator plugin make dependency to that module -->

		<!--<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
		<plugin type="org.mybatis.generator.plugins.ToStringPlugin" />-->
		<!--<plugin type="org.mybatis.generator.plugins.FieldNamePlugin" />-->
		<!--<plugin type="purchase.plugins.ValueOfPlugin" />-->

		<jdbcConnection driverClass="org.h2.Driver"
			connectionURL="jdbc:h2:file:./target/h2/gen/db" userId="sa" password="" />

		<javaModelGenerator targetPackage="com.acme.gen.domain" targetProject="MAVEN">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
			<property name="rootClass" value="com.acme.db.domain.Base" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.acme.gen.mapper" targetProject="MAVEN">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator targetPackage="com.acme.gen.mapper" targetProject="MAVEN" type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<table tableName="purchase_order" />
		<table tableName="content" >
			<columnOverride column="content" javaType="byte[]" typeHandler="base64" />
			<columnOverride column="is_default" javaType="boolean" typeHandler="yn" />
		</table>
		<table tableName="person" />
		<table tableName="company" />
		<table tableName="item" />
		<table tableName="item_content" />
		<table tableName="order_items" />
		<table tableName="company_employers" />
		<table tableName="category" />
		<table tableName="credentials" />
	</context>
</generatorConfiguration>
